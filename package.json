{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "mongoose-plugins-accessible-by",
  "description": "A mongoose plugin that adds role based security information to a schema",
  "version": "0.2.0",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/mongoose-plugins-accessible-by.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./test"
  },
  "keywords" : [
    "mongoose","roles","security","plugin"
  ],
  "scripts": {
    "watch" : "coffee -c -w  -o lib src",
    "test": "NODE_ENV=test node_modules/.bin/mocha --timeout 10000 -R spec test/*.coffee"
  },
  "engines": {
    "node": ">=0.6.0"
  },
  "dependencies": {
    "underscore" : "1.4.x",
    "mongoose" : "3.x",
    "pkginfo" : "0.2.x"
  },
  "devDependencies": {
    "async" : "0.1.22",
    "should" : "1.2.0",   
    "coffee-script" : "1.3.3", 
    "mocha": "1.6.0",
    "database-cleaner": "0.7.0",
    "mongoskin": "0.4.4"

  },
  "bugs": { "url": "http://github.com/codedoctor/mongoose-plugins-accessible-by/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/codedoctor/mongoose-plugins-accessible-by/raw/master/LICENSE"
    }
  ]
  
}
